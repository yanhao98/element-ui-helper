{
  "name": "element-ui-helper",
  "version": "1.0.18-beta.1",
  "description": "A helper for element-ui",
  "type": "module",
  "author": "",
  "scripts": {
    "dev": "vite",
    "build": "npm run clean && npm run type-check && vite build",
    "clean": "rm -rf lib && rm -rf types",
    "preview": "pnpm run build && vite preview",
    "type-check": "vue-tsc --noEmit --skipLibCheck -p tsconfig.app.json",
    "prepublishOnly": "npm run build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "repository": {
    "url": "https://github.com/yanhao98/element-ui-helper"
  },
  "files": [
    "lib"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.3",
    "@vitejs/plugin-vue2": "^2.3.1",
    "@vitejs/plugin-vue2-jsx": "^1.1.1",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.4.0",
    "@vue/tsconfig": "^0.5.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-dts": "^3.7.1",
    "vitepress": "1.0.0-rc.36",
    "vue-tsc": "^1.8.27"
  },
  "dependencies": {
    "element-ui": "^2.15.14",
    "lodash": "^4.17.21",
    "vue": "^2.7.14",
    "vue-request": "^2.0.4"
  },
  "stackblitz": {
    "startCommand": "pnpm dev"
  }
}